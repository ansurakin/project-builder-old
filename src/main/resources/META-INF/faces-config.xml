<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_2.xsd"
              version="2.2">
    <application>
        
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
        
        <!--<navigation-handler>com.alex.development.tools.project.builder.gui.navigation.MyNavigationHandler</navigation-handler>-->
        
        <resource-bundle>
            <base-name>bundle.message</base-name>
            <var>msg</var>
        </resource-bundle>
        
        <converter>
            <converter-id>urlCatalogConverter</converter-id>
            <converter-class>com.alex.development.tools.project.builder.gui.converter.UrlCatalogConverter</converter-class>
        </converter>
        
    </application>
</faces-config>